  "components": {
    "schemas": {
      "SuccessHealthCheckResponseBody": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string",
                "description": "Status atual da aplicação",
                "example": "HEALTH"
              },
              "stage": {
                "type": "string",
                "description": "Ambiente da aplicação",
                "example": "DEVELOPMENT"
              },
              "info": {
                "type": "object",
                "properties": {
                  "database": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "description": "Status do banco de dados",
                        "example": "UP"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "ErrorHealthCheckResponseBody": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string",
                "description": "Status atual da aplicação",
                "example": "UNHEALTH"
              },
              "stage": {
                "type": "string",
                "description": "Ambiente da aplicação",
                "example": "DEVELOPMENT"
              },
              "info": {
                "type": "object",
                "properties": {
                  "database": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "string",
                        "description": "Status do banco de dados",
                        "example": "DOWN"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "CreateCustomerRequestBody": {
        "type": "object",
        "properties": {
          "document": {
            "type": "string",
            "description": "CPF do cliente",
            "example": "12345678911"
          },
          "email": {
            "type": "string",
            "description": "E-mail do cliente",
            "example": "teste@email.com"
          },
          "name": {
            "type": "string",
            "description": "Nome do cliente",
            "example": "Teste"
          }
        },
        "required": [
          "document",
          "email",
          "name"
        ]
      },
      "CreateCustomerResponseBody": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object",
