              "id": {
                "type": "string",
                "description": "Id do cliente criado",
                "example": "610e5de2-9d0b-44c2-ba7b-5d872cd5a067"
              }
            }
          }
        }
      },
      "GetCustomerByDocumentResponseBody": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Id do cliente",
                "example": "610e5de2-9d0b-44c2-ba7b-5d872cd5a067"
              },
              "document": {
                "type": "string",
                "description": "CPF do cliente",
                "example": "12345678911"
              },
              "name": {
                "type": "string",
                "description": "Nome do cliente",
                "example": "Teste"
              },
              "email": {
                "type": "string",
                "description": "E-mail do cliente",
                "example": "teste@email.com"
              },
              "createdAt": {
                "type": "string",
                "description": "Data de criação do cliente",
                "example": "2024-05-23T03:23:07.956Z"
              },
              "updatedAt": {
                "type": "string",
                "description": "Data da última atualização do cliente",
                "example": "2024-05-23T03:23:07.956Z"
              }
            }
          }
        }
      },
      "SaveProductRequestBody": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Nome do produto",
            "example": "Cheeseburger"
          },
          "description": {
            "type": "string",
            "description": "Descrição do produto",
            "example": "Pão, hambúrguer de carne, queijo, alface, tomate e maionese"
          },
          "price": {
            "type": "double",
            "description": "Valor do produto",
            "example": 14.99
          },
          "category": {
            "type": "string",
            "enum": [
              "LANCHE",
              "ACOMPANHAMENTO",
              "BEBIDA",
              "SOBREMESA"
            ],
            "description": "Categoria do produto",
            "example": "LANCHE"
          },
          "imageLinks": {
            "type": "string[]",
            "description": "Lista de imagens do produto",
            "example": [
              "https://exemplo.com/lanche1",
              "https://exemplo.com/lanche2"
            ]
          }
        },
        "required": [
          "name",
          "description",
          "price",
          "category",
          "imageLinks"
        ]
      },
      "SaveProductResponseBody": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "int",
                "description": "Id do produto criado",
                "example": 1
              }
            }
          }
        }
      },
      "SearchProductsResponseBody": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "int",
                  "description": "Id do produto",
                  "example": 1
                },
                "name": {
                  "type": "string",
                  "description": "Nome do produto",
                  "example": "Cheeseburger"
                },
                "description": {
                  "type": "string",
                  "description": "Descrição do produto",
                  "example": "Pão, hambúrguer de carne, queijo, alface, tomate e maionese"
                },
                "price": {
                  "type": "double",
                  "description": "Valor do produto",
                  "example": 14.99
                },
                "category": {
                  "type": "string",
                  "enum": [
                    "LANCHE",
                    "ACOMPANHAMENTO",
                    "BEBIDA",
                    "SOBREMESA"
                  ],
                  "description": "Categoria do produto",
                  "example": "LANCHE"
                },
                "imageLinks": {
                  "type": "string[]",
                  "description": "Lista de imagens do produto",
                  "example": [
                    "https://exemplo.com/lanche1",
                    "https://exemplo.com/lanche2"
                  ]
                },
                "createdAt": {
                  "type": "string",
                  "description": "Data de criação do produto",
                  "example": "2024-05-23T03:23:07.956Z"
                },
                "updatedAt": {
                  "type": "string",
                  "description": "Data da última atualização do produto",
                  "example": "2024-05-23T03:23:07.956Z"
                }
              }
            }
          }
        }
      },
      "MakeCheckoutRequestBody": {
        "type": "object",
        "properties": {
          "customerId": {
            "type": "string",
            "description": "Id do cliente",
            "example": "1c014ba2-4000-4d40-be1e-352e29d07d89"
          },
          "items": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "int",
                  "description": "Id do produto",
                  "example": 1
                },
                "quantity": {
                  "type": "int",
                  "description": "Quantidade do produto",
                  "example": 1
                },
                "observation": {
                  "type": "string",
                  "description": "Observações gerais no produto",
                  "example": "Tirar cebola"
                }
              }
            }
          },
          "orderAmount": {
            "type": "double",
            "description": "Valor total do pedido",
            "example": 14.99
          },
          "payment": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
