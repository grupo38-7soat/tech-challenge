{
  "name": "project",
  "version": "0.0.1",
  "main": "main.ts",
  "repository": "git@github.com:grupo20-7soat/tech-challenge.git",
  "author": "Grupo 20",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "export NODE_ENV=development && ts-node-dev -r tsconfig-paths/register ./src/bootstrap.ts",
    "start": "npm run build && node dist/src/bootstrap.js",
    "start:prod": "export NODE_ENV=production && npm run start",
    "test": "jest --noStackTrace --runInBand --passWithNoTests --silent --forceExit",
    "test:watch": "jest --noStackTrace --runInBand --passWithNoTests --verbose --watch",
    "test:cov": "npm run test -- --coverage --no-cache",
    "lint": "npx eslint --fix --ignore-path .eslintignore --ext .js,.ts ."
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "express": "^4.19.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  }
}
